type Mutation {
    createBook(title: String!, pages: Int!, author: String!, gender: String!): Book
}

type Query{
    findAllBooks: [Book!]
}

type Book{
    id: ID!
    title: String!
    pages: Int!
    author: String!
    gender: String!
    reviews: [Review]
}

type Review {
   id: ID!
    title: String!
    comment: String!
}
